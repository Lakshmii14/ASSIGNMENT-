let str1 = "Hello, ";
let str2 = "World!";

// Concatenate using the concat method
let concatenatedWithMethod = str1.concat(str2);
console.log("Concatenated using concat method:", concatenatedWithMethod);  // Prints: Concatenated using concat method: Hello, World!

// Concatenate using the + operator
let concatenatedWithOperator = str1 + str2;
console.log("Concatenated using + operator:", concatenatedWithOperator);
